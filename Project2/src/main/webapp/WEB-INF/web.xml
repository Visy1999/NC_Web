<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">


  <display-name>Created Web Application</display-name>


  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>viewUser.EditUser.jsp</welcome-file>
    <welcome-file>viewUser.User.jsp</welcome-file>
    <welcome-file>viewCompany.Company.jsp</welcome-file>
    <welcome-file>viewCompany.EditCompany.jsp</welcome-file>
  </welcome-file-list>



  <servlet>
    <servlet-name>ComoanySerEdit</servlet-name>
    <servlet-class>Servlets.CompanyServlet.CompanyEditServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ComoanySerEdit</servlet-name>
    <url-pattern>/CompanyEdit</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>ComoanySer</servlet-name>
    <servlet-class>Servlets.CompanyServlet.CompanyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ComoanySer</servlet-name>
    <url-pattern>/Company</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ComoanySerD</servlet-name>
    <servlet-class>Servlets.CompanyServlet.CompanyDeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ComoanySerD</servlet-name>
    <url-pattern>/cd</url-pattern>
  </servlet-mapping>




  <servlet>
    <servlet-name>UserSer</servlet-name>
    <servlet-class>Servlets.UserServlet.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserSer</servlet-name>
    <url-pattern>/User</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>UserSerD</servlet-name>
    <servlet-class>Servlets.UserServlet.UserDeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserSerD</servlet-name>
    <url-pattern>/d</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>UserSerE</servlet-name>
    <servlet-class>Servlets.UserServlet.UserEditServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserSerE</servlet-name>
    <url-pattern>/EditUser</url-pattern>
  </servlet-mapping>


</web-app>
